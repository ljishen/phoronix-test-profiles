<?xml version="1.0"?>
<!--Phoronix Test Suite v7.0.0-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>Latt</Title>
    <AppVersion>0.1</AppVersion>
    <Description>Simple latency tester that combines multiple processes.</Description>
    <ResultScale>ms</ResultScale>
    <Proportion>LIB</Proportion>
    <SubTitle>min_wait=100ms, max_wait=500ms</SubTitle>
    <Executable>latt-run</Executable>
    <TimesToRun>3</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>1.0.0</Version>
    <SupportedPlatforms>Linux, MacOSX, BSD</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>Processor</TestType>
    <License>Free</License>
    <Status>Verified</Status>
    <ExternalDependencies>build-utilities</ExternalDependencies>
    <Extends>pts/video-sample</Extends>
    <EnvironmentSize>1.0</EnvironmentSize>
    <ProjectURL>http://git.kernel.dk/cgit/latt/</ProjectURL>
    <InternalTags>SMP</InternalTags>
    <Maintainer>Jianshen Liu</Maintainer>
  </TestProfile>
  <TestSettings>
    <Option>
      <DisplayName>Number Clients</DisplayName>
      <Identifier>number-clients</Identifier>
      <ArgumentPrefix>-c</ArgumentPrefix>
      <ArgumentPostfix></ArgumentPostfix>
      <DefaultEntry>0</DefaultEntry>
      <Menu>
        <Entry>
          <Name>4</Name>
          <Value>4</Value>
          <Message></Message>
        </Entry>
        <Entry>
          <Name>8</Name>
          <Value>8</Value>
          <Message></Message>
        </Entry>
        <Entry>
          <Name>16</Name>
          <Value>16</Value>
          <Message></Message>
        </Entry>
        <Entry>
          <Name>32</Name>
          <Value>32</Value>
          <Message></Message>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
